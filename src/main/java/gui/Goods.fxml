<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<TableView fx:id="goodsTable" editable="true" onMouseClicked="#onTableClicked" prefHeight="600.0" prefWidth="1150.0"
           tableMenuButtonVisible="true" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
           fx:controller="gui.GoodsList">
    <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="131.0" text="Name" fx:id="colName"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="113.0" text="Supply" fx:id="colRealSupply"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="113.0" text="Actual bought" fx:id="colActualBought"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="124.0" text="Consumption" fx:id="colConsumption"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="129.0" text="Affordable" fx:id="colAffordable"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="124.0" text="Max demand" fx:id="colMaxDemand"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="97.0" text="Base price" fx:id="colBasePrice"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="97.0" text="Min price" fx:id="colMinPrice"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="97.0" text="Max price" fx:id="colMaxPrice"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="124.0" text="Market price" fx:id="colPrice"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="50.0" text="Trend" fx:id="colTrend"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="160.0" text="Overproduced %" fx:id="colOverproduced"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="160.0" text="1-Day Volatility %" fx:id="colFluctuation"/>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="80.0" text="Inflation %" fx:id="colInflation"/>
    </columns>
</TableView>
